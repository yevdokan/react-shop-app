  "license": "MIT",
  "homepage": ".",
  "scripts": {
    "dev": "next dev",
    "build": "next build",
    "start": "next start",
    "commit": "git-cz"
  },
  "dependencies": {
    "@ant-design/icons": "^4.6.3",
    "axios": "^0.21.1",
    "classnames": "^2.3.1",
    "core-js": "^3.16.2",
    "crypto-js": "^4.1.1",
    "moment": "^2.29.1",
    "next": "11.1.0",
    "next-compose-plugins": "^2.2.1",
