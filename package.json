    "react-dom": "17.0.2",
    "react-images": "^1.2.0-beta.7",
    "react-markdown": "^7.0.0",
    "react-motion": "^0.5.2",
    "react-redux": "^7.2.4",
    "redux": "^4.1.1",
    "redux-actions": "^2.6.5",
    "redux-persist": "^6.0.0",
    "redux-thunk": "^2.3.0",
    "sass": "^1.38.1"
