    "stylelint-config-recommended": "^5.0.0",
    "stylelint-order": "^4.1.0",
    "typescript": "^4.3.5"
  },
  "config": {
    "commitizen": {
      "path": "node_modules/cz-conventional-changelog"
    }
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged && ls-lint",
      "commit-msg": "commitlint -e $HUSKY_GIT_PARAMS"
    }
  },
  "lint-staged": {
    "*{.js,.jsx,.ts,.tsx}": [
      "eslint --fix",
      "prettier --write",
      "git add"
    ],
    "*{.css,.scss}": [
      "stylelint --fix",
      "prettier --write",
      "git add"
    ]
  }
